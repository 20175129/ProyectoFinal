@model ProyectoFinalv2.Models.MantenimientoEmpleado

@{
    ViewBag.Title = "Eliminar";
}

<h2>Eliminar</h2>

<h3>¿Estás seguro que quieres eliminar a este Empleado?</h3>
<div>
    <h4>Módulo de Mantenimiento de Empleados</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
           <h5 class="control-label col-md-2"> Codigo </h5>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.CodigoEmpleado)
        </dd>

        <dt>
            <h5 class="control-label col-md-2"> Nombre </h5>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NombreEmpleado)
        </dd>

        <dt>
            <h5 class="control-label col-md-2"> Apellido </h5>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ApellidoEmpleado)
        </dd>

        <dt>
            <h5 class="control-label col-md-2"> Telefono </h5>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TelefonoEmpleado)
        </dd>

        <dt>
            <h5 class="control-label col-md-2"> Departamento </h5>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DepartamentoEmpleado)
        </dd>

        <dt>
            <h5 class="control-label col-md-2"> Cargo </h5>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CargoEmpleado)
        </dd>

        <dt>
            <h5 class="control-label col-md-2"> Fecha de Ingreso </h5>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.FechaIngresoEmpleado)
        </dd>

        <dt>
            <h5 class="control-label col-md-2"> Salario </h5>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SalarioEmpleado)
        </dd>

        <dt>
            <h5 class="control-label col-md-2"> Estatus </h5>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Estatus)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Eliminar" class="btn btn-default" /> |
            @Html.ActionLink("Regresar", "Index")
        </div>
    }
</div>
